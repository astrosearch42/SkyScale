QGroupBox {{
    border: 1.5px solid #e3e8f0;
    border-radius: 12px;
    margin-top: 8px;
    background: #f3f6fb;
    padding-top: 10px;
}}
QGroupBox::title {{
    background: transparent;
    subcontrol-origin: margin;
    left: 14px;
    padding: 0 6px;
    color: #2563eb;
    font-weight: 600;
}}
